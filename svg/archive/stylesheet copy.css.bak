@import url("https://fonts.googleapis.com/css?family=Roboto"); 
@import url("https://fonts.googleapis.com/css?family=Roboto+Condensed"); 
@import url("https://fonts.googleapis.com/css?family=Inter"); 

@font-face {
   font-family: 'jznato';
   src:  url("https://jzedwards.github.io/fonts/jznato-v11-webfont.woff2") format('woff2'),
         url("https://jzedwards.github.io/fonts/jznato-v11-webfont.woff") format('woff'),
         url("https://jzedwards.github.io/fonts/jznato-v11.otf") format('opentype'); 
}
@font-face {
   font-family: 'europa';
   src:  url("https://jzedwards.github.io/fonts/europa-regular.v2-webfont.woff2") format('woff2'),
         url("https://jzedwards.github.io/fonts/europa-regular.v2.woff") format('woff'),
         url("https://jzedwards.github.io/fonts/europa-regular.v2.otf") format('opentype'); 
}
@font-face {
   font-family: 'Univers';
   src:  url("https://jzedwards.github.io/fonts/univers-webfont.woff2") format('woff2');
}

@font-face {
   font-family: 'UniversCN';
   src:  url("https://jzedwards.github.io/fonts/universCN-webfont.woff2") format('woff2');
}
@import url("https://fonts.googleapis.com/css?family=Inter"); 
body { font-family:'Inter'; } 
h5 { font-family: 'europa'; font-size: 144px; font-weight: 400;}
h6 { font-family: 'jznato'; font-size: 144px; font-weight: 400;}
